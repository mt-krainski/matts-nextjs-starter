{
  "name": "matts-starter",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run prepare-third-party-licenses-file && cp THIRD_PARTY_LICENSES.txt public/ && next build && node ./scripts/add-copyright-notice.js",
    "start": "next start",
    "lint": "next lint",
    "update-email-templates": "dotenv -e .env -- node scripts/update-email-templates.js",
    "prepare-third-party-licenses-file": "node ./scripts/parse-licenses.js",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:push": "supabase db push",
    "supabase:pull": "supabase db pull",
    "supabase:diff": "supabase db diff --schema public",
    "supabase:types": "supabase gen types typescript --linked > src/types/database.types.ts",
    "supabase:migration:new": "supabase migration new",
    "supabase:migration:list": "supabase migration list",
    "supabase:migrate": "npm run supabase:push && npm run supabase:types"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.53.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.534.0",
    "next": "15.4.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "license-checker": "^25.0.1",
    "supabase": "^2.33.5",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5"
  }
}
